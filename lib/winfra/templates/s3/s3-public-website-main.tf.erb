variable "<%= @sanitized_domain %>_bucket_name" {
  default     = "<%= @domain %>"
  description = "Bucket name"
}

module "s3_<%= @sanitized_domain %>" {
  source = "./<%= @env %>/s3/<%= @domain %>"
  bucket_name = "${var.<%= @sanitized_domain %>_bucket_name}"
  acl = "<%= @public_website ? "public-read" : "private" %>"
}
