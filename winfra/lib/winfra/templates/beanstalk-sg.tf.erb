resource "aws_security_group" "beanstalk" {
  name        = "beanstalk-${var.environment}-sg"
  description = "Beanstalk EC2 Security Group"
  <% if @has_vpc %>
    vpc_id    = "${var.vpc_id}"
  <% end %>

  egress {
    from_port   = 0
    to_port     = 0
    protocol    = "-1"
    cidr_blocks = ["0.0.0.0/0"]
  }
}
