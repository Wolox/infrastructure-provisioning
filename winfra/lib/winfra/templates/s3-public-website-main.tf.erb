variable "bucket_name" {
  default     = "<%= @domain %>"
  description = "Bucket name"
}

module "s3" {
  source = "../../modules/s3"
  bucket_name = "${var.bucket_name}"
  acl = "public-read"
  has_policy = true

  website {
    index_document = "index.html"
    error_document = "error.html"
  }
}
